public class libros {
    private String titulo;
    private String Autor;

    public libros(String titulo, String auto) {
        this.titulo = titulo;
        this.Autor = Autor;
    }
    public String getTitulo() {
        return titulo;
    }
    public String getAutor() {
        return Autor;
    }
    public String toString() {
        return "titulo=" + titulo + ", autor=" + Autor;

    }

    }
